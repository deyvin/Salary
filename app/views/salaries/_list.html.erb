<table id="salaries" class="table table-striped table-bordered">
	<thead>
		<th>Estado</th>
		<th>Cidade</th>
		<th>Salário</th>
	</thead>
	<tbody>
		<% @salarios.each do |city, amount| %>
			<tr>
				<td><%= city.state.name %> </td>
				<td><%= city.name %> </td>
				<td class="coluna_salario" title="salário programador ruby/rails em <%=city.state.name %> - <%= city.name%>"><%= number_to_currency(amount, format: "%u %n", separator: ",", delimiter: ".", unit: "R$") %> </td>
			</tr>
		<% end %>
	</tbody>
</table>